<%= semantic_form_for [:admin, @song] do |f| %>
  <%= f.inputs do %>
    <%= f.input :track_no %>
    <%= f.input :name %>
    <%= f.input :artist %>
    <%= f.input :album %>
    <%= f.input :categories %>
    <%=  cl_upload_tag(:video_id, :resource_type => "video", :html => {:id => "my_upload_tag"}) %>
  <% end %>
  <%= f.actions :submit %>
<% end %>